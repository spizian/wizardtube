{
  "name": "spizian/laravel-wizard-installer",
  "description": "Laravel wizard installer package",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Adi miuprix",
      "email": "adimiuprix@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "spizian\\wizardtube\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spizian\\wizardtube\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "illuminate/support": "^10.8",
    "spizian/laravel-env-set": "^1.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.5",
    "nunomaduro/larastan": "^2.6",
    "pestphp/pest": "^2.5",
    "pestphp/pest-plugin-laravel": "^2.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "spizian\\wizardtube\\wizardtubeServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
